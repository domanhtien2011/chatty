<h1 class="text-sm-center">All People</h1>
<table class="table table-inverse">
  <thead>
    <tr>
      <th>Name</th>
      <th>Action</th>
    </tr>
  </thead>
  <% User.all.each do |user| %>
  <tbody>
    <tr>
      <% if current_user.id != user.id %>
        <td><%= user.username %></td>
        <td><%= link_to "Add to friendlist", friendships_path(:friend_id => user), method: :post %></td>
    </tr>
      <% end %>
  </tbody>
  <% end %>
</table>
